// Place your key bindings in this file to override the defaultsauto[]
[
    // GENERAL PROGRAMIC KEYS - compSciFund RELATED - PYTHON PROGRAMMING  
    //IMP-NOTE: All mathematical symb will follow the pattern of `cmd+shift`
    //IMP-NOTE: All frontend pseudo will follow the pattern of `cmd+alt`

    {
        "key": "ctrl+r",
        "command": "code-runner.run"
    },
    {
        "key": "ctrl+alt+n",
        "command": "-code-runner.run"
    }, 

    {
        "key": "ctrl+[", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "[]_"
        }
    },
    {
        "key": "ctrl+t", 
        "command": "workbench.action.toggleMaximizedPanel", 
    },

    {
        "key": "ctrl+\\", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "〈|〉"
        }
    },
    {
        "key": "ctrl+.", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "|〉"
        }
    },
    {
        "key": "ctrl+,", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "〈|"
        }
    },
    {
        "key": "ctrl+o", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "O(n)_t: \nO(n)_s: "
        }
    },
    {
        "key": "ctrl+;", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "::"
        }
    },
    {
        "key": "ctrl+shift+b", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "∋"
        }
    },
    {
        "key": "ctrl+shift+c", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "∈"
        }
    },
    {
        "key": "ctrl+alt+p", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "print('\\n----------[ START ]----------\\n')\nprint()\nprint('\\n----------[ END ]----------\\n') "
        }
    },
    {
        "key": "ctrl+d", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "''' \n\tArgs:\n\t\t\n\tReturns:\n\t\t\n'''"
        }
    },
    {
        "key": "ctrl+space", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "_"
        }
    },
    {
        "key": "ctrl+'", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "'''  '''"
        }
    },


    {
        "key": "ctrl+n", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "NOTE"
        }
    },
    {
        "key": "alt+right",
        "command": "cursorWordEndRight",
        "when": "textInputFocus && !accessibilityModeEnabled"
    },
    {
        "key": "alt+left",
        "command": "cursorWordEndLeft",
        "when": "textInputFocus && !accessibilityModeEnabled"
    },
    {
        "key": "ctrl+right",
        "command": "cursorLineEnd"
    },
    {
        "key": "ctrl+left",
        "command": "cursorLineStart"
    },
    // END - GENERAL PROGRAMIC KEYS - compSciFund RELATED - PYTHON PROGRAMMING 

    // START - GENERAL LIQUID KEYS 
    {
        "key": "ctrl+shift+f", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "{% for %}\n\t\n{% endfor %}"
        }
    },
    {
        "key": "ctrl+shift+i", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "{% if %}\n\n{% endif %}"
        }
    },
    {
        "key": "ctrl+shift+[", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "{%  %}"
        }
    },
    {
        "key": "ctrl+shift+p", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "\n{% comment %} UI TEST {% endcomment %}\n\t<script> \n\t\tprint({{  | json }})\n\t</script>\n{% comment %} UI TEST {% endcomment %}\n"
        }
    },
    {
        "key": "ctrl+shift+a", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "{% assign  %}"
        }
    },
    // START - GENERAL LIQUID KEYS 

    // START - GENERAL MARKDOWN KEYS 


    {
        "key": "alt+,", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "<></> "
        }
    },


    {
        "key": "alt+d", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "<span style='color:#00CED1; line-height: 2.0;'>\n \n \n</span>"
        }
    },
    {
        "key": "alt+s", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "<span></span>"
        }
    },
    {
        "key": "alt+i", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "<center><img src=''/></center>"
        }
    },
    {
        "key": "alt+c", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "<span style='color:#3FFF00; font-family: Courier New;'></span>"
        }
    },
    {
        "key": "alt+shift+c", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "## <center><span style='color:#3FFF00; font-family: Courier New;'></span></center>"
        }
    },
    // END - GENERAL MD KEYS 

    // START - GENERIC JAVASCRIPT KEYS 
    {
        "key": "ctrl+shift+d", 
        "command": "editor.action.insertSnippet", 
        "when": "editorTextFocus", 
        "args": {
            "snippet": "/**\n@description: \n@param {} \n**/"
        }
    },
    // {
    //     "key": "ctrl+'", 
    //     "command": "editor.action.insertSnippet", 
    //     "when": "editorTextFocus", 
    //     "args": {
    //         "snippet": "/**\n\t \n**/"
    //     }
    // },
    // {
    //     "key": "ctrl+alt+d", 
    //     "command": "editor.action.insertSnippet", 
    //     "when": "editorTextFocus", 
    //     "args": {
    //         "snippet": "\n\n/******************************[ START ]********************************/\n// -----------------------[  ]-----------------------\n/***********************************************************************/\n\n/***********************************************************************/\n// -----------------------[  ]-----------------------\n/*******************************[ END ]*********************************/\n\n"
    //     }
    // },
    // {
    //     "key": "ctrl+alt+p", 
    //     "command": "editor.action.insertSnippet", 
    //     "when": "editorTextFocus", 
    //     "args": {
    //         "snippet": "console.log('\\n----------[ START ]----------\\n')\nconsole.log()\nconsole.log('\\n----------[ END ]----------\\n') "
    //     }
    // },
    {
        "key": "alt+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "f11",
        "command": "-workbench.action.toggleFullScreen",
        "when": "!isIOS"
    },
    {
        "key": "f1",
        "command": "-workbench.action.showCommands"
    },
    {
        "key": "ctrl+shift+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "f8",
        "command": "-editor.action.marker.nextInFiles",
        "when": "editorFocus"
    },
    {
        "key": "f8",
        "command": "extension.prettifyJSON"
    },
    {
        "key": "ctrl+shift+p",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "shift+f2",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    // END - GENERIC JAVASCRIPT KEYS 
]