    "jupyter.colorizeInputBox": false,
    "notebook.insertToolbarLocation": "betweenCells",
    "notebook.showCellStatusBar": "hidden",
    "notebook.cellToolbarVisibility": "hover",
    "material-icon-theme.folders.associations": {
        "my_virtual_env": "vm",
        "DSnA":"graphql"
    },
